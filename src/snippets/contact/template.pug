.contact 
    .contact__container 
        .contact__form
            form(action="")
                .form__item
                    label.form__label(for="cname") Nome
                    input#cname.form__input.form__input--text(type="text", placeholder="SEU NOME", name="cname")
                .form__item 
                    label.form__label(for="cemail") E-Mail
                    input#cemail.form__input.form__input--email(type="email", placeholder="SEU E-MAIL", name="cemail")
                .form__div
                    .form__item 
                        label.form__label(for="ccompany") Empresa
                        input#ccompany.form__input.form__input--text(type="text", placeholder="EMPRESA", name="ccompany")
                    .form__item 
                        label.form__label(for="cphone") Telefone
                        input#cphone.form__input.form__input--phone(type="text", placeholder="TELEFONE", name="ccocphone")
                .form__div
                    .form__item 
                        label.form__label(for="ccity") Cidade
                        input#ccity.form__input.form__input--phone(type="text", placeholder="CIDADE", name="ccity")
                    .form__item 
                        label.form__label(for="cstate") Estado
                        input#cstate.form__input.form__input--phone(type="text", placeholder="ESTADO", name="cstate")
                .form__actions 
                    .form__submit
                        button.button.button--action-invert Enviar 
        .contact__help 
            .contact__social 
                .contact__social-title 
                    h4.title.title--h2 Siga a Vooxer
                .contact__social-list 
                    .contact__social-item 
                        a.contact__social-link(href="#facebook")
                            +svg('facebook', 40, 40)
                    .contact__social-item 
                        a.contact__social-link(href="#twitter")
                            +svg('twitter', 40, 40)
                    .contact__social-item 
                        a.contact__social-link(href="#instagram")
                            +svg('instagram', 40, 40)
                    .contact__social-item 
                        a.contact__social-link(href="#youtube")
                            +svg('youtube', 40, 40)
                    .contact__social-item 
                        a.contact__social-link(href="#linkedin")
                            +svg('linkedin', 40, 40)

            .contact__box 
                .contact__name 
                    h6.title.title--h6 VOOXER 
                .contact__text
                    p R. Adão Stropa, 200
                    p Jardim Acapulco, 
                    p Marília - SP
                    p 17525-180

